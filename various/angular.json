{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "education": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/education",
      "sourceRoot": "projects/education/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/education",
            "index": "projects/education/src/index.html",
            "main": "projects/education/src/main.ts",
            "polyfills": "projects/education/src/polyfills.ts",
            "tsConfig": "projects/education/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/education/src/favicon.ico",
              "projects/education/src/assets"
            ],
            "styles": [
              "projects/education/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/education/src/environments/environment.ts",
                  "with": "projects/education/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "education:build:production"
            },
            "development": {
              "browserTarget": "education:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "education:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/education/src/test.ts",
            "polyfills": "projects/education/src/polyfills.ts",
            "tsConfig": "projects/education/tsconfig.spec.json",
            "karmaConfig": "projects/education/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/education/src/favicon.ico",
              "projects/education/src/assets"
            ],
            "styles": [
              "projects/education/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
